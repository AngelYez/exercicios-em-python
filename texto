frase = 'Toronto Canada'
print(len(frase))
print(frase.count('o', 0, 6 ))
print(frase.find('ronto'))
print(frase.find('him'))
print('Canada' in frase)
print('him' in frase)
print(frase.replace('Toronto', 'eu no'))
print(frase.upper())
print(frase.lower())
print(frase.capitalize())
print(frase.title())
print(frase.strip())
print(frase.lstrip())
print(frase.rstrip())
lista = frase.split()
print(' '.join(frase.split()))
